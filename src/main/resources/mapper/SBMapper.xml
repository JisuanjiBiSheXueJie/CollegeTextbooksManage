<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gydx.bookManager.mapper.SBMapper">
    <update id="updateBookIdByStockOutId" parameterType="integer">
        update stockout_book set book_id = #{bookId} where stock_out_id = #{stockOutId};
    </update>
</mapper>